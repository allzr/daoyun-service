<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fzu.edu.CourseMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.fzu.edu.daoyun.entity.Course">
        <id column="couID" property="couID" />
        <result column="name" property="name" />
        <result column="lastEditorID" property="lastEditorID" />
        <result column="lastEditTime" property="lastEditTime" />
        <result column="delete" property="isDelete" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        couID, name, lastEditorID, lastEditTime, isDelete
    </sql>

</mapper>
